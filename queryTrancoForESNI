#!/usr/bin/env bash

filename='/home/zisis/Downloads/tranco_YJLG.csv'
#filename='/home/zisis/Desktop/Programming/domains'
n=1
while read line; 
do
   # reading each line
   echo "Line No. $n : $line"
   #IFS=', ' read -r -a array << "$line"
   IFS=',' read -ra array <<< "$line"
   echo "-----------------"
   echo "FIrst part : ${array[0]}"
   echo "SEcod part : ${array[1]}"
   echo "-----------------"
   url='https://www.'${array[1]}
   if eval /home/zisis/Desktop/Programming/curl/curl-esni '$url'; then #2>&1 | grep "handshake, Finished"; then
      echo "Successful handshake!!"
   else 
      echo "Failure in handshake!!"
   fi
done < $filename
